# External Services (if needed)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_key_here

# Development Settings
VITE_ENABLE_DEBUG_MODE=true
VITE_LOG_LEVEL=info 